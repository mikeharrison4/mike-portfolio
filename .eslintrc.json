{
  "extends": ["next/core-web-vitals", "airbnb"],
  "rules": {
    "quotes": ["error", "single"],
    "react/jsx-props-no-spreading": "off",
    "react/jsx-filename-extension": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "no-tabs": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
